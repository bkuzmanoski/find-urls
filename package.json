{
  "name": "find-urls",
  "version": "0.1.1",
  "description": "An intelligent, zero-dependency utility to find and normalize URLs in text, with advanced punctuation and context handling.",
  "author": "Brian Kuzmanoski <bkuzmanoski@icloud.com> (https://github.com/bkuzmanoski)",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/find-urls.min.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:umd": "esbuild src/index.ts --bundle --outfile=dist/umd/find-urls.min.js --format=iife --global-name=findUrls --minify",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:umd",
    "lint": "eslint . --ext .ts",
    "test": "tsx --test test/index.test.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "keywords": [
    "url",
    "link",
    "uri",
    "href",
    "extract",
    "find",
    "get",
    "scan",
    "parse",
    "text",
    "string",
    "content",
    "punctuation",
    "trim",
    "domain",
    "tld",
    "localhost",
    "protocol",
    "unicode",
    "idn",
    "regex",
    "isomorphic",
    "universal",
    "browser",
    "node",
    "lightweight",
    "zero-dependency",
    "configurable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkuzmanoski/find-urls.git"
  },
  "homepage": "https://github.com/bkuzmanoski/find-urls#readme",
  "bugs": {
    "url": "https://github.com/bkuzmanoski/find-urls/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^18.19.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
